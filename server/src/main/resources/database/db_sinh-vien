CREATE DATABASE full_stack

USE full_stack

CREATE TABLE sinh_vien(
                          id bigint primary key AUTO_INCREMENT not null,
                          ten varchar(50),
                          tuoi int,
                          gioi_tinh bit,
                          chuyen_nganh varchar(50)
)
select *from sinh_vien
